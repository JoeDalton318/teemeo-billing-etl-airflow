# Airflow custom image with all dependencies pre-installed
FROM apache/airflow:2.8.1-python3.11

USER airflow

# Install all required Python packages
RUN pip install --no-cache-dir \
    pandas==2.0.3 \
    psycopg2-binary==2.9.9 \
    boto3==1.33.13 \
    scikit-learn==1.3.2 \
    pyarrow==14.0.2 \
    openpyxl==3.1.5

USER airflow
